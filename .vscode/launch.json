    "version": "0.2.0",
    "configurations": [
        {
            "name": "Dynamo",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/dynamo/launcher.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Dynamo Tests",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "args": [
                "tests",
                "--asyncio-mode=strict",
            ],
            "console": "integratedTerminal"
        }
    ]
}
